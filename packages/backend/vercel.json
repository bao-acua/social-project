{
  "version": 2,
  "buildCommand": "cd ../shared && npm install && npm run build && cd ../backend && npm install && npm run build",
  "installCommand": "echo 'Install handled in buildCommand'",
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
